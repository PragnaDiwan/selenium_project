package pages;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;


public class bswpom {
	
	WebDriver driver;
	
	By img=By.xpath("//span[@class='visible-xl visible-lg visible-md hidden-xs visible-sm']//img");
	
	public bswpom(WebDriver driver)
	{
		this.driver=driver;
	}

	
	public void bswclick()
	{
		
		
		driver.findElement(img).click();
		
		
		
	}
	
	
	
}



